<div class="rightCol">
  <div>
    <p style="float: right;"><%= link_to 'New Event', new_admin_event_path %></p>
    <h2>Events (<%= @events.size %>)</h2>
  </div>
  <table>
    <tr>
      <th>Title</th>
    </tr>
    <% @events.each do |event| %>
      <tr class="<%= cycle("even", "odd") -%>">
        <td><%= link_to "#{event.name}", edit_admin_event_path(event) %></td>
      </tr>
    <% end -%>
  </table>
  <br />

  <%= will_paginate(@events) %>

</div>
