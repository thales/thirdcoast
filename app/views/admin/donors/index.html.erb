<div class="rightCol">
  <div>
    <p style="float: right;">
      <%= link_to 'Reorder', reorder_admin_donors_path, :rel => "facebox" %>
      |
      <%= link_to 'New Corporate Donor', new_admin_donor_path %>
    </p>
    <h2>Corporate Donors (<%= @donors.size %>)</h2>
  </div>
  <table id="donors">
    <tr>
      <th>Title</th>

    </tr>
    <% @donors.each do |donor| %>
      <tr class="<%= cycle("even", "odd") -%>">
        <td><%= link_to "#{donor.name}", edit_admin_donor_path(donor) %></td>
      </tr>
    <% end -%>
  </table>
  <br />

  <%= will_paginate(@donors) %>

</div>
