<div class="rightCol">
  <h1>New corporate Donor</h1>

  <% form_for([:admin, @donor], :html => { :multipart => true} ) do |form| %>
    <%= render :partial => 'form', :locals => {:form => form} %>
    <p><%= form.submit 'Create' %></p>
  <% end %>

  <p><%= link_to 'Back', admin_donors_path %></p>
</div>