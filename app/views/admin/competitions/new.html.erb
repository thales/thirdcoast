<div class="rightCol">


<h1>New competition</h1>

<% form_for([:admin,@competition]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Cancel', admin_competitions_path %>
</div>